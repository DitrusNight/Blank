class Item(x: u8) {
    virtual fn test() => {
        x = 5;
        x
    };
    fn test2() => x;
    fn test3() => test2();
    fn test4() => {
        this.x = 2;
        this.test2()
    };
    fn test5() => {
        test()
    };
    fn test6() => {
        this.x = 2;
        this.test()
    }
};

fn main() => {
    let item = Item(1);
    item.x = 2;
    item.x + item.test() + item.test2()
};
